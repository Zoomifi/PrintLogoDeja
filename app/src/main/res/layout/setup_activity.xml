<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/customer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#E0E0E0"
    android:gravity="center_horizontal"
    tools:context=".MerchantTestActivity">
    <RelativeLayout
        android:id="@+id/actionbar"
        android:background="@color/AquaGreen"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:orientation="horizontal"
        android:layout_alignParentTop="true"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Deja Vu"
            android:textSize="26sp"
            android:paddingRight="10dp"
            android:paddingLeft="20dp"
            android:layout_centerVertical="true"
            android:textColor="@color/White"
            android:layout_alignParentLeft="true"/>

        <Button
            android:id="@+id/ipe_button"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_toLeftOf="@+id/bar_button"
            android:text="IPEs"
            android:textSize="20sp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            android:background="@drawable/gray_button"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            />
        <Button
            android:id="@+id/bar_button"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_toLeftOf="@+id/door_button"
            android:text="Bar"
            android:textSize="20sp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            android:background="@drawable/gray_button"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            />
        <Button
            android:id="@+id/door_button"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_toLeftOf="@+id/bleft"
            android:text="Door"
            android:textSize="20sp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            android:background="@drawable/gray_button"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            />

        <ImageButton
            android:id="@+id/bleft"
            android:layout_toLeftOf="@+id/pages"
            android:layout_height="50dp"
            android:layout_width="50dp"
            android:scaleType="fitCenter"
            android:background="@android:color/transparent"
            android:layout_centerVertical="true"
            android:src="@drawable/ic_action_left" />
        <TextView
            android:id="@+id/pages"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/bright"
            android:layout_centerVertical="true"
            android:text="Page 1"
            android:textSize="26sp"
            android:textStyle="bold"
            android:textColor="@color/White"
            />
        <ImageButton
            android:id="@+id/bright"
            android:layout_toLeftOf="@+id/home_button"
            android:layout_height="50dp"
            android:layout_width="50dp"
            android:scaleType="fitCenter"
            android:background="@android:color/transparent"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/ic_action_right" />

        <Button
            android:id="@+id/home_button"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_alignParentRight="true"
            android:text="Home"
            android:textSize="20sp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            android:background="@drawable/gray_button"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/buttonView"
        android:layout_below="@+id/actionbar"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_centerHorizontal="true">
        <LinearLayout
            android:id="@+id/row1"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">

            <Button
                android:id="@+id/button1"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

            <Button
                android:id="@+id/button2"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

            <Button
                android:id="@+id/button3"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />
            <Button
                android:id="@+id/button4"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/row2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">

            <Button
                android:id="@+id/button5"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

            <Button
                android:id="@+id/button6"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

            <Button
                android:id="@+id/button7"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />
            <Button
                android:id="@+id/button8"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/row3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">

            <Button
                android:id="@+id/button9"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

            <Button
                android:id="@+id/button10"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />
            <Button
                android:id="@+id/button11"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

            <Button
                android:id="@+id/button12"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/row4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">

            <Button
                android:id="@+id/button13"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

            <Button
                android:id="@+id/button14"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />
            <Button
                android:id="@+id/button15"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

            <Button
                android:id="@+id/button16"
                android:layout_width="220dp"
                android:layout_height="100dp"
                android:text="@string/OtherAmount"
                android:textSize="26sp"
                android:textColor="@color/White"
                android:textStyle="bold"
                android:background="@drawable/aqua_green_button"
                android:layout_margin="5dp"
                android:padding="5dp"
                />

        </LinearLayout>
    </LinearLayout>
    <Button
        android:id="@+id/select_button"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="Go Back"
        android:textSize="20sp"
        android:textColor="@color/White"
        android:textStyle="bold"
        android:background="@drawable/red_button"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_margin="20dp"
        android:padding="10dp"
        />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="* Modifiers Will Not Be Included"
        android:textSize="22sp"
        android:textColor="@color/Black"
        android:paddingRight="20dp"
        android:layout_marginBottom="10dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="* Fixed Price Items Only"
        android:textSize="22sp"
        android:textColor="@color/Black"
        android:paddingRight="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="15dp"/>
    </LinearLayout>
    <FrameLayout
        android:id="@+id/full_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </FrameLayout>
</RelativeLayout>
